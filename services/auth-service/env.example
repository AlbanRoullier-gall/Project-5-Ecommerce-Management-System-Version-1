# Configuration Auth Service
NODE_ENV=development
PORT=3001

# Configuration JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# Configuration Base de données
DB_HOST=localhost
DB_PORT=5432
DB_NAME=auth_service_db
DB_USER=auth_user
DB_PASSWORD=auth_password

# Note: L'auth-service NE communique PAS directement avec d'autres services
# Toute orchestration est gérée par l'API Gateway
